t\\css\\_layout.scss","name":null,"original":{"line":136,"column":8},"generated":{"line":176,"column":18}}],"sources":{"pages\\account\\account.styles.scss":"@import '../../core/content/css/breakpoint';\r\n@import '../../core/content/css/palette';\r\n@import '../../core/content/css/components';\r\n@import '../../core/content/css/layout';\r\n\r\n.root {\r\n  @include cabecera ($palette-primary-main, $palette-primary-contrast-text);\r\n\r\n  @include navmenu ($palette-primary-main, $palette-secondary-main, $palette-secondary-contrast-text);\r\n\r\n  & .title-container {\r\n    @include contenedor-titulo($palette-secondary-main);\r\n  }\r\n\r\n  & form {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: flex-start;\r\n    gap: 16px;\r\n\r\n    @include campo-txt-con-error($palette-error-main);\r\n\r\n    & .submit-button {\r\n      @include button;\r\n    }\r\n\r\n    & fieldset {\r\n      @include fieldSet($palette-secondary-main);\r\n    }\r\n\r\n    & .field.select {\r\n      @include select-con-error($palette-error-main);\r\n    }\r\n\r\n    & .field-inline-container {\r\n      @include contenedor-campos-form\r\n    }\r\n  }\r\n\r\n  @include pie($palette-secondary-main);\r\n  // <# TODO: Move styles to common >\r\n}\r\n","core\\content\\css\\_layout.scss":"\r\n@mixin cabecera($bg-colour, $contrast-text) { \r\n  display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    & > :nth-child(n){\r\n        width: 100%;\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n\r\n        & > :nth-child(n){\r\n            max-width: $breakpoints-lg;\r\n            width: 100%;/*para que ocupe todo lo que pueda*/\r\n        }\r\n    }\r\n\r\n      & header{\r\n        background-color: $bg-colour;\r\n        padding: 40px 0;\r\n\r\n        & .header-subcontainer{\r\n            display: flex;\r\n            flex-direction: row;\r\n            justify-content: space-between;\r\n            align-items: center;\r\n            padding: 0 16px;\r\n\r\n            & img{\r\n                width:100%;\r\n                max-width: 200px;\r\n            }\r\n        }\r\n        & .user{\r\n            color: $contrast-text;\r\n            display: flex;\r\n            flex-direction: row;\r\n            gap: 10px;\r\n            &::before{\r\n                content: \"\";\r\n                background: url(\"../../core/content/img/icon-user.svg\") no-repeat center center;\r\n                width: 20px;\r\n            }\r\n\r\n        }\r\n    }\r\n      \r\n}\r\n\r\n@mixin navmenu ($colour-1, $colour-2, $colour-2-contrast-text){\r\n  & nav{\r\n    background-color: $colour-2;\r\n  }\r\n    \r\n    & .navbar-list{\r\n        list-style: none;\r\n        margin: 10px 0;\r\n        padding: 0;\r\n        display: flex;\r\n        flex-direction: row;\r\n        justify-content: center;\r\n        gap: 40px;\r\n\r\n        & > :nth-child(n){\r\n            color: $colour-2-contrast-text;\r\n\r\n            & a{\r\n                color: $colour-2-contrast-text;\r\n                text-decoration: none;\r\n            }\r\n        }\r\n\r\n        & .selected{\r\n            border-bottom: 5px solid $colour-1;\r\n            padding-bottom: 5px;\r\n        }\r\n    } \r\n}\r\n@mixin contenedor-titulo($colour-2) {\r\n  display: flex;\r\n        flex-direction: row;\r\n        justify-content: space-between;\r\n        border-bottom: 5px solid $colour-2;\r\n        padding-bottom: 5px;\r\n        margin-top: 30px;\r\n\r\n        & .title{\r\n            margin: 0;\r\n            font-weight: 300;\r\n            font-size: 2.2rem;\r\n        }\r\n}\r\n\r\n@mixin cabecera-contenido($color){\r\n  width: 100%;\r\n  margin-top: 5px;\r\n  color: $color;\r\n}\r\n\r\n@mixin tabla{\r\n  & thead tr {\r\n    background-color: #ccc;\r\n    font-weight: 900;\r\n  }\r\n\r\n  & tbody tr:nth-child(even) {\r\n    background-color: #eee;\r\n    font-weight: 300;\r\n  }\r\n\r\n  & td {\r\n    padding: 10px;\r\n    font-size: 0.8rem;\r\n  }\r\n\r\n  & .align-right {\r\n    text-align: right;\r\n  }\r\n}\r\n\r\n\r\n\r\n@mixin pie ($colour-2){\r\n  & footer {\r\n    background-color: $colour-2;\r\n    padding: 80px;\r\n    margin-top: 100px;\r\n\r\n    & .footer-subcontainer {\r\n      display: flex;\r\n      flex-direction: row;\r\n      justify-content: center;\r\n      \r\n      & img {\r\n        width: 100%;\r\n        max-width: 200px;\r\n      }\r\n    }\r\n  }\r\n}","core\\content\\css\\_breakpoint.scss":"$breakpoints-sm: 600px;\r\n$breakpoints-md: 900px;\r\n$breakpoints-lg: 1200px;\r\n$breakpoints-xl: 1920px;","core\\content\\css\\_palette.scss":"$palette-primary-main:#22c6cb;\r\n$palette-primary-contrast-text:white;\r\n$palette-secondary-main:#232c3b;\r\n$palette-secondary-contrast-text:white;\r\n$palette-error-main:#f44336;","core\\content\\css\\_components.scss":"@import \"./palette\";\r\n@mixin contenedor-campos-form {\r\n  width: 100%;\r\n      display: flex;\r\n      flex-direction: row;\r\n      align-items: center;\r\n      gap: 20px;\r\n\r\n      & label {\r\n        min-width: 300px;\r\n        text-align: right;\r\n      }\r\n\r\n      & .field {\r\n        max-width: 400px;\r\n      }\r\n\r\n      & .field.select {\r\n        max-width: 200px;\r\n      }\r\n\r\n      & .error-message.error {\r\n        margin-left: 0;\r\n      }\r\n}\r\n@mixin button($color: $palette-primary-main, $contrast-text: $palette-primary-contrast-text) {\r\n    background-color: $color;\r\n    color: $contrast-text;\r\n    border: none;\r\n    padding: 12px 24px;\r\n    border-radius: 10px;\r\n    cursor: pointer;\r\n    font-weight: 900;\r\n    transition: all ease 0.2s;\r\n    text-decoration:none;\r\n  \r\n    &:hover {\r\n      background-color: darken($color, 10%);\r\n    }\r\n  \r\n    &:active {\r\n      box-shadow: inset 0 2px 0 0 rgba(0, 0, 0, 0.1),\r\n       inset 0 2px 2px 2px rgba(0, 0, 0, 0.2),\r\n       inset 0 0 2px 2px rgba(0, 0, 0,